{
  "outputDirectory": ".",
  "buildCommand": "echo 'Static build - no build needed'",
  "rewrites": [
    {
      "source": "/instachecker/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/instachecker",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/instachecker/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
