{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/instachecker/:path*",
      "destination": "/:path*"
    }
  ],
  "headers": [
    {
      "source": "/instachecker/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
